{
  "name": "@fuchsia/cli",
  "version": "0.0.1",
  "description": "CLI tool for FuchsiaJS - The declarative JSX/TSX Nodejs framework built on express ",
  "main": "dist/bin/index.js",
  "bin": {
    "fnc": "dist/bin/index.js"
  },
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc",
    "format": "prettier ./bin/index.ts --write",
    "lint": "eslint ./bin/index.ts",
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prebuild": "npm run format && npm run clean",
    "fnc:dev": "ts-node ./bin/index.ts",
    "fnc:prod": "node ./dist/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phl3bas/fennec-cli.git"
  },
  "keywords": [
    "cli",
    "fuchsiajs",
    "typescript",
    "express",
    "nodejs",
    "JSX",
    "TSX"
  ],
  "author": "Sepp Wikramaratna-Clarke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phl3bas/fennec-cli/issues"
  },
  "homepage": "https://github.com/Phl3bas/fennec-cli#readme",
  "devDependencies": {
    "@types/core-js": "^2.5.3",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.21",
    "jest": "^26.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^5.1.0",
    "glob": "^7.1.6",
    "inquirer": "^7.3.0",
    "ora": "^4.0.4",
    "prettier": "^2.0.5",
    "yargs": "^15.4.0"
  }
}
